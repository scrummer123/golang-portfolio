{
    "compilerOptions": {
        "moduleResolution": "node",
        "outDir": "./src/build/",
        "noImplicitAny": true,
        "module": "esnext",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "sourceMap": true,
        "removeComments": true
    },
    "include": ["./src/**/**/*"]
}
